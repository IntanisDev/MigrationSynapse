{
	"name": "SAP_DM_TIMEPERIOD_CON",
	"properties": {
		"folder": {
			"name": "Datalake_Levis/Datos Maestros/02 Consumption"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DM_TIMEPERIOD_ST",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
