				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TimePeriodId as string,",
				"          Date as string,",
				"          DateName as string,",
				"          DayOfMonth as string,",
				"          DayOfMonthName as string,",
				"          HalfYear as string,",
				"          HalfYearDate as string,",
				"          HalfYearName as string,",
				"          Month as string,",
				"          MonthDate as string,",
				"          MonthName as string,",
				"          PreviousDate as string,",
				"          PreviousHalfYearDate as string,",
				"          PreviousHalfYearName as string,",
				"          PreviousMonthDate as string,",
				"          PreviousMonthName as string,",
				"          PreviousTrimesterDate as string,",
				"          PreviousTrimesterName as string,",
				"          PreviousWeekDate as string,",
				"          PreviousWeekName as string,",
				"          PreviousYear as string,",
				"          PreviousYearDate as string,",
				"          PreviousYearName as string,",
				"          Quarter as string,",
				"          QuarterDate as string,",
				"          QuarterName as string,",
				"          Trimester as string,",
				"          TrimesterDate as string,",
				"          TrimesterName as string,",
				"          Week as string,",
				"          WeekDate as string,",
				"          WeekName as string,",
				"          WeekRange as string,",
				"          Year as string,",
				"          YearDate as string,",
				"          YearName as string,",
				"          FiscalHalfYear as string,",
				"          FiscalMonthDate as string,",
				"          FiscalMonthName as string,",
				"          FiscalQuarterName as string,",
				"          FiscalWeekName as string,",
				"          FiscalYear as string,",
				"          FiscalWeekDate as string,",
				"          InicioMesFical as string,",
				"          ant1 as string,",
				"          ant2 as string,",
				"          w_ant1 as string,",
				"          InicioQuarterFiscal as string,",
				"          q_ant1 as string,",
				"          InicioAnoFiscal as string,",
				"          y_ant1 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(TimePeriodId = toDate(TimePeriodId,'yyyy-MM-dd'),",
				"          DayOfMonth = toInteger(DayOfMonth),",
				"          HalfYear = toInteger(HalfYear),",
				"          Month = toInteger(Month),",
				"          MonthDate = toDate(MonthDate,'yyyy-MM-dd'),",
				"          PreviousDate = toDate(PreviousDate,'yyyy-MM-dd'),",
				"          PreviousMonthDate = toDate(PreviousMonthDate,'yyyy-MM-dd'),",
				"          PreviousWeekDate = toDate(PreviousWeekDate,'yyyy-MM-dd'),",
				"          PreviousYear = toInteger(PreviousYear),",
				"          PreviousYearDate = toDate(PreviousYearDate,'yyyy-MM-dd'),",
				"          Quarter = toInteger(Quarter),",
				"          Trimester = toInteger(Trimester),",
				"          TrimesterDate = toDate(TrimesterDate,'yyyy-MM-dd'),",
				"          Week = toInteger(Week),",
				"          WeekDate = toDate(WeekDate,'yyyy-MM-dd'),",
				"          Year = toInteger(Year),",
				"          YearDate = toDate(YearDate,'yyyy-MM-dd'),",
				"          FiscalWeekDate = toDate(FiscalWeekDate,'yyyy-MM-dd'),",
				"          InicioMesFiscal = toDate(InicioMesFical,'yyyy-MM-dd'),",
				"          Ant1 = toDate(ant1,'yyyy-MM-dd'),",
				"          Ant2 = toDate(ant2,'yyyy-MM-dd'),",
				"          W_Ant1 = toDate(w_ant1,'yyyy-MM-dd'),",
				"          InicioQuarterFiscal = toDate(InicioQuarterFiscal,'yyyy-MM-dd'),",
				"          Q_Ant1 = toDate(q_ant1,'yyyy-MM-dd'),",
				"          InicioAnoFiscal = toDate(InicioAnoFiscal,'yyyy-MM-dd'),",
				"          Y_Ant1 = toDate(y_ant1,'yyyy-MM-dd')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     truncate: true,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          TimePeriodId,",
				"          Date,",
				"          DateName,",
				"          DayOfMonth,",
				"          DayOfMonthName,",
				"          HalfYear,",
				"          HalfYearDate,",
				"          HalfYearName,",
				"          Month,",
				"          MonthDate,",
				"          MonthName,",
				"          DayOfMonthName,",
				"          PreviousDate,",
				"          PreviousHalfYearDate,",
				"          PreviousHalfYearName,",
				"          PreviousMonthDate,",
				"          PreviousMonthName,",
				"          PreviousTrimesterDate,",
				"          PreviousTrimesterName,",
				"          PreviousWeekDate,",
				"          PreviousWeekName,",
				"          PreviousYear,",
				"          PreviousYearDate,",
				"          PreviousYearName,",
				"          Quarter,",
				"          QuarterDate,",
				"          QuarterName,",
				"          Trimester,",
				"          TrimesterDate,",
				"          TrimesterName,",
				"          Week,",
				"          WeekDate,",
				"          WeekName,",
				"          WeekRange,",
				"          Year,",
				"          YearDate,",
				"          YearName,",
				"          FiscalHalfYear,",
				"          FiscalMonthDate,",
				"          FiscalMonthName,",
				"          FiscalQuarterName,",
				"          FiscalWeekName,",
				"          FiscalYear,",
				"          FiscalWeekDate,",
				"          InicioMesFiscal,",
				"          Ant1 = derivedColumn1@Ant1,",
				"          Ant2 = derivedColumn1@Ant2,",
				"          W_Ant1 = derivedColumn1@W_Ant1,",
				"          InicioQuarterFiscal,",
				"          Q_Ant1 = derivedColumn1@Q_Ant1,",
				"          InicioAnoFiscal,",
				"          Y_Ant1 = derivedColumn1@Y_Ant1",
				"     )) ~> sink1"
			]
		}
	}
}